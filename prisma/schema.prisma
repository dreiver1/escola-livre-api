  junho_idjunho                                      Int
  julho_idjulho                                      Int
  agosto_idagosto                                    Int
  setembro_idsetembro                                Int
  setembro_idsetembro1                               Int
  outubro_idoutro                                    Int
  novembro_idnovembro                                Int
  dezembro_iddezembro                                Int
  alunos_idAluno                                     Int
  abril                                              abril     @relation(fields: [abril_idabril], references: [idabril], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_abril1")
  agosto                                             agosto    @relation(fields: [agosto_idagosto], references: [idagosto], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_agosto1")
  alunos                                             alunos    @relation(fields: [alunos_idAluno], references: [idAluno], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_alunos1")
  dezembro                                           dezembro  @relation(fields: [dezembro_iddezembro], references: [iddezembro], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_dezembro1")
  fevereiro                                          fevereiro @relation(fields: [fevereiro_idfevereiro], references: [idfevereiro], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_fevereiro1")
  janeiro                                            janeiro   @relation(fields: [janeiro_idjaneiro], references: [idjaneiro], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_janeiro1")
  julho                                              julho     @relation(fields: [julho_idjulho], references: [idjulho], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_julho1")
  junho                                              junho     @relation(fields: [junho_idjunho], references: [idjunho], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_junho1")
  maio                                               maio      @relation(fields: [maio_idmaio], references: [idmaio], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_maio1")
  marco                                              marco     @relation(fields: [marco_idmarco], references: [idmarco], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_marco1")
  novembro                                           novembro  @relation(fields: [novembro_idnovembro], references: [idnovembro], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_novembro1")
  outubro                                            outubro   @relation(fields: [outubro_idoutro], references: [idoutro], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_outubro1")
  setembro_Frequencia_setembro_idsetembroTosetembro  setembro  @relation("Frequencia_setembro_idsetembroTosetembro", fields: [setembro_idsetembro], references: [idsetembro], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_setembro1")
  setembro_Frequencia_setembro_idsetembro1Tosetembro setembro  @relation("Frequencia_setembro_idsetembro1Tosetembro", fields: [setembro_idsetembro1], references: [idsetembro], onDelete: NoAction, onUpdate: NoAction, map: "fk_Frequencia_setembro2")

  @@index([abril_idabril], map: "fk_Frequencia_abril1_idx")
  @@index([agosto_idagosto], map: "fk_Frequencia_agosto1_idx")
  @@index([alunos_idAluno], map: "fk_Frequencia_alunos1_idx")
  @@index([dezembro_iddezembro], map: "fk_Frequencia_dezembro1_idx")
  @@index([fevereiro_idfevereiro], map: "fk_Frequencia_fevereiro1_idx")
  @@index([janeiro_idjaneiro], map: "fk_Frequencia_janeiro1_idx")
  @@index([julho_idjulho], map: "fk_Frequencia_julho1_idx")
  @@index([junho_idjunho], map: "fk_Frequencia_junho1_idx")
  @@index([maio_idmaio], map: "fk_Frequencia_maio1_idx")
  @@index([marco_idmarco], map: "fk_Frequencia_marco1_idx")
  @@index([novembro_idnovembro], map: "fk_Frequencia_novembro1_idx")
  @@index([outubro_idoutro], map: "fk_Frequencia_outubro1_idx")
  @@index([setembro_idsetembro], map: "fk_Frequencia_setembro1_idx")
  @@index([setembro_idsetembro1], map: "fk_Frequencia_setembro2_idx")
}

